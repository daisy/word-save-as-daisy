<Window x:Class="DaisyAddin.CustomActions.CustomActionProgress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DaisyAddin.CustomActions"
        mc:Ignorable="d" Width="300">
    <StackPanel>
        <Label x:Name="ProgressMessage" Height="30">Downloading DAISY Pipeline app</Label>
        <ProgressBar x:Name="DownloadProgressBar"
                     VerticalAlignment="Top"
                     Margin="10,10,10,10"
                     Height="30" />
    </StackPanel>
</Window>
