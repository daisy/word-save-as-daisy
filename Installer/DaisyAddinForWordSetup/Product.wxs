    <!--  IMPORTANT NOTE:
              When creating a new release, 
                - update the AddinVersion variable
                - set the value of Product/@Id to a new GUID (without curly brackets, letters all upper-case)
                - set the value of Package/@Id to a new GUID
              Note that GUID values should be all upper-case and without brackets 
        -->
    
    <!-- Preprocessor variables, update following your addin assemblies version -->
    <?define AddinVersion = "2.9.2.0" ?>
    
    <!-- DO NOT touch this code -->
    <?define UpgradeCode = "81FEF910-4CDE-43B9-8E7B-9F7D34A14765"?>
    
    <!-- Variable to build either x64 or x86 package with the same wix project -->
    <?if $(var.Platform) = x64 ?>
        <?define Win64 = "yes" ?>
        <?define PlatformProgramFilesFolder = "ProgramFiles64Folder" ?>
    <?else ?>
        <?define Win64 = "no" ?>
        <?define PlatformProgramFilesFolder = "ProgramFilesFolder" ?>
    <?endif ?>
    
    <!--Change this ID for update -->
