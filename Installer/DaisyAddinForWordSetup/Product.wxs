<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:util="http://schemas.microsoft.com/wix/UtilExtension">
    <!--  IMPORTANT NOTE:
              When creating a new release, 
                - update the AddinVersion variable
                - set the value of Product/@Id to a new GUID (without curly brackets, letters all upper-case)
                - set the value of Package/@Id to a new GUID
              Note that GUID values should be all upper-case and without brackets 
        -->
    
    <!-- Preprocessor variables, update following your addin assemblies version -->
    <?define AddinVersion = "2.8.5.0" ?>
    
    <!-- DO NOT touch this code -->
    <?define UpgradeCode = "81FEF910-4CDE-43B9-8E7B-9F7D34A14765"?>
    
    <!-- Variable to build either x64 or x86 package with the same wix project -->
    <?if $(var.Platform) = x64 ?>
        <?define Win64 = "yes" ?>
        <?define PlatformProgramFilesFolder = "ProgramFiles64Folder" ?>
    <?else ?>
        <?define Win64 = "no" ?>
        <?define PlatformProgramFilesFolder = "ProgramFilesFolder" ?>
    <?endif ?>
    
    <!--Change this ID for update -->
    <Product Id="0FC3774A-D792-4DC8-834C-94135F364F5A" 
            Name="SaveAsDAISY add-in version $(var.AddinVersion)" 
            Language="1033" Version="$(var.AddinVersion)" 
            Manufacturer="DAISY Consortium" 
            UpgradeCode="$(var.UpgradeCode)">
        <!--Change this ID for update -->
        <Package Id="AF5B13CC-71EA-4669-AEDD-55658A86394D"
				 Compressed="yes"
				 InstallPrivileges="limited"
				 InstallerVersion="200" />
        <Media Id="1" Cabinet="Daisy.cab" EmbedCab="yes" />
        <Media Id="2" Cabinet="pipeline.cab" EmbedCab="no" CompressionLevel="high"/>
        <!-- remove previous versions of previous Word add-in -->
        
        <Upgrade Id="4EB56DA5-96B7-48AA-BEE1-4BDCDFF54105">
            <UpgradeVersion Property="OLDAPPFOUND_WORD" OnlyDetect="no" IncludeMinimum="yes" Minimum="0.0.0" IncludeMaximum="no" Maximum="2.0.10" />
        </Upgrade>
        
        <Upgrade Id="$(var.UpgradeCode)">
            <UpgradeVersion Property="OLDAPPFOUND" Minimum="0.0.0.0" Maximum="$(var.AddinVersion)" IncludeMinimum="yes" IncludeMaximum="yes" />
            <UpgradeVersion Property="NEWAPPFOUND" Minimum="$(var.AddinVersion)" IncludeMinimum="no" OnlyDetect="yes" />
        </Upgrade>


        <!-- Custom actions dll, from the DaisyAddin.CustomActions project -->
        <Binary Id="DaisyAddinCustomActions" SourceFile="$(var.DaisyAddin.CustomActions.TargetDir)\DaisyAddin.CustomActions.CA.dll" />

        <!--wix variables-->
        <WixVariable Id="VersionNumber" Value="$(var.AddinVersion)" />
        <WixVariable Id="VersionInfo" Value="Version !(wix.VersionNumber)" />
        <!--Properties section-->
        <Icon Id="icon.ico" SourceFile="$(var.SolutionDir)Lib/speaker.ico" />
        <Property Id="ARPPRODUCTICON" Value="icon.ico" />
        <!--<Property Id="WIXUI_INSTALLDIR" Value="APPLICATIONFOLDER" />-->
        <CustomAction Id="SetARPREADME" Property="ARPREADME" Value="[#Readme]" />
        <Property Id="ARPCOMMENTS" Value="$(var.AddinVersion)" />
        <Property Id="ARPCONTACT" Value="DAISY Consortium" />
        <Property Id="ARPURLINFOABOUT" Value="https://daisy.org/" />
        <Property Id="ARPHELPLINK" Value="https://daisy.github.io/word-save-as-daisy" />
        <Property Id="WixShellExecTarget" Value="[#SaveAsDAISY_Instruction_Manual_July_2023.docx]" />
        <Property Id="SHOW_README" Value="1" />
        <!-- Property set by DetectLatestMsWordVersion in the DaisyAddin.CustomAction project 
            Default assumes office 2007 (12.0) 32bits -->
        <Property Id="LATESTWORDVERSION">"12.0"</Property>
        <Property Id="LATESTWORDISX64">"no"</Property>

		<Property Id="ApplicationFolderName" Value="Save-as-DAISY Word Addin" />
        

        <Condition Message="Please install Windows XP Service Pack 3">
            <![CDATA[Installed OR (VersionNT > 501) OR (VersionNT = 501 AND ServicePackLevel >= 3)]]>
        </Condition>
